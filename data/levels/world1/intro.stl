      (oneshot #t)
      (script "Tux.deactivate();
Tux.set_dir(false);
Effect.sixteen_to_nine(1);
wait(1);
pause_music(5);
Text.set_text(\"Tux had found a letter, but what he read had shocked him...\");
Text.fade_in(1);
wait(5);
Text.fade_out(1);
wait(2);
fade_in_music(\"music/present/level3/airship3.music\", 2);
letter.fade_color(1, 1, 1, 1, 2);
wait(7);
letter.fade_color(1, 1, 1, 0, 2);
wait(2.1);
Tux.walk(50);
Tux.do_jump(-400);
wait(1);
Tux.walk(0);
Text.set_text(\"Tux didn't like Nolok for a while now, ever since the kart racing, but this just made things personal.\");
Text.fade_in(1);
wait(6);
Text.fade_out(1);
wait(1.5);
Tux.do_jump(-400);
Text.set_text(\"So that's when Tux decided... he was going to go on an adventure to Crocodile Swamp so he could defeat Nolok once and for all!\");
Text.fade_in(1);
wait(7);
Effect.fade_out(2);
wait(3);
Level.finish(true);")
